{
    "name": "maxsky/umeng-service",
    "description": "UMeng Service",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Max Sky",
            "email": "max940213@live.com"
        }
    ],
    "require": {
        "php": "^7.2.5|^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-sodium": "*",
        "guzzlehttp/guzzle": "^7.4",
        "maxsky/aliyun-api-gateway-sdk": "^1.2",
        "phpseclib/phpseclib": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "UMeng\\": "src/"
        },
        "files": [
            "src/Constants/API_URL.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
      "optimize-autoloader": true,
      "preferred-install": "dist",
      "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
